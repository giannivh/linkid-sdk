	<name>SafeOnline Tests</name>
    <description>
        Contains integration and acceptance tests for the
        SafeOnline service.
    </description>

	<groupId>net.lin-k.safe-online</groupId>
	<artifactId>safe-online-test</artifactId>
	<version>1.1</version>
	<packaging>jar</packaging>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<configuration>
					<testFailureIgnore>true</testFailureIgnore>
				</configuration>
			</plugin>
		</plugins>
	</build>
    <profiles>
		<profile>
			<id>MacOSX</id>
			<activation>
				<os>
					<name>mac os x</name>
				</os>
			</activation>
			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-eclipse-plugin</artifactId>
						<configuration>
							<classpathContainers>
								<classpathContainer><![CDATA[org.eclipse.jdt.launching.JRE_CONTAINER/org.eclipse.jdt.internal.launching.macosx.MacOSXType/JVM 1.6]]></classpathContainer>
							</classpathContainers>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
    </profiles>

