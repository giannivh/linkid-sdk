<?xml version="1.0" encoding="UTF-8"?><project>
  <parent>
    <artifactId>safe-online</artifactId>
    <groupId>net.lin-k</groupId>
    <version>1.0.0-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>net.lin-k.safe-online</groupId>
  <artifactId>safe-online-performance-agent-service</artifactId>
  <packaging>sar</packaging>
  <name>SafeOnline Performance Agent Service</name>
  <version>1.0-SNAPSHOT</version>
  <description>Agent for the SafeOnline Performance Testing.</description>
  <build>
    <plugins>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>jboss-sar-maven-plugin</artifactId>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>jboss-maven-plugin</artifactId>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>net.lin-k.safe-online</groupId>
      <artifactId>safe-online-container</artifactId>
      <version>1.0-SNAPSHOT</version>
      <type>pom</type>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>net.lin-k.safe-online</groupId>
      <artifactId>safe-online-performance-scenario</artifactId>
      <version>1.0-SNAPSHOT</version>
      <type>ejb-client</type>
      <exclusions>
        <exclusion>
          <artifactId>safe-online-performance-keystore</artifactId>
          <groupId>net.lin-k.safe-online</groupId>
        </exclusion>
        <exclusion>
          <artifactId>safe-online-model-performance</artifactId>
          <groupId>net.lin-k.safe-online</groupId>
        </exclusion>
        <exclusion>
          <artifactId>safe-online-performance-entity</artifactId>
          <groupId>net.lin-k.safe-online</groupId>
        </exclusion>
        <exclusion>
          <artifactId>safe-online-performance-scenario-base</artifactId>
          <groupId>net.lin-k.safe-online</groupId>
        </exclusion>
        <exclusion>
          <artifactId>safe-online-performance-scenario-keystore</artifactId>
          <groupId>net.lin-k.safe-online</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>net.lin-k.safe-online</groupId>
      <artifactId>safe-online-j2ee-util</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>
    <dependency>
      <groupId>net.lin-k.safe-online</groupId>
      <artifactId>safe-online-performance-console-base</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>
    <dependency>
      <groupId>commons-logging</groupId>
      <artifactId>commons-logging</artifactId>
      <exclusions>
        <exclusion>
          <artifactId>servlet-api</artifactId>
          <groupId>javax.servlet</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>log4j</groupId>
      <artifactId>log4j</artifactId>
    </dependency>
  </dependencies>
  <reporting>
    <plugins>
      <plugin>
        <artifactId>maven-changes-plugin</artifactId>
        <reportSets>
          <reportSet>
            <reports>
              <report>changes-report</report>
            </reports>
          </reportSet>
        </reportSets>
      </plugin>
      <plugin>
        <artifactId>maven-javadoc-plugin</artifactId>
      </plugin>
      <plugin>
        <artifactId>maven-surefire-report-plugin</artifactId>
      </plugin>
    </plugins>
  </reporting>
</project>