<?xml version="1.0" encoding="UTF-8"?>
<!-- vi:set ts=4 sw=4 noexpandtab: -->

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">
	<application>
		<resource-bundle>
			<base-name>messages.webapp</base-name>
			<var>msgs</var>
		</resource-bundle>
		<locale-config>
			<default-locale>en</default-locale>
			<supported-locale>nl</supported-locale>
		</locale-config>
		<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
	</application>
	<lifecycle>
		<phase-listener>org.jboss.seam.jsf.SeamPhaseListener</phase-listener>
		<phase-listener>
			net.link.safeonline.webapp.listener.LanguagePhaseListener</phase-listener>
	</lifecycle>
	<navigation-rule>
		<navigation-case>
			<from-outcome>login-success</from-outcome>
			<to-view-id>/overview.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>logout-success</from-outcome>
			<to-view-id>/main.seam</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/applications.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>view-application</from-outcome>
			<to-view-id>/application-view.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>edit-application</from-outcome>
			<to-view-id>/application-edit.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>viewstats</from-outcome>
			<to-view-id>/stats.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>edit-usage-agreement</from-outcome>
			<to-view-id>/usage-agreements.seam</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/application-view.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/applications.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>edit-application</from-outcome>
			<to-view-id>/application-edit.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>view-usage-agreement</from-outcome>
			<to-view-id>/usage-agreement-view.seam</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/application-edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/applications.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>saved</from-outcome>
			<to-view-id>/applications.seam</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/stats.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/applications.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>viewstat</from-outcome>
			<to-view-id>/stat-view.seam</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/stat-view.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/stats.seam</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/usage-agreement-view.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>viewtext</from-outcome>
			<to-view-id>/usage-agreement-text-view.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/application-view.seam</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/usage-agreement-text-view.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/usage-agreement-view.seam</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/usage-agreements.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>edittext</from-outcome>
			<to-view-id>/usage-agreement-text-edit.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>viewtext</from-outcome>
			<to-view-id>/usage-agreement-text-view.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/applications.seam</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/usage-agreement-text-edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/usage-agreements.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>saved</from-outcome>
			<to-view-id>/usage-agreements.seam</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
</faces-config>