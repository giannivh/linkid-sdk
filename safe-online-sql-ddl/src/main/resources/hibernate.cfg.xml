<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD//EN"
"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<property name="dialect">
			org.hibernate.dialect.MySQLDialect
		</property>
		<mapping class="net.link.safeonline.entity.SubjectEntity" />
		<mapping class="net.link.safeonline.entity.ApplicationEntity" />
		<mapping
			class="net.link.safeonline.entity.ApplicationOwnerEntity" />
		<mapping class="net.link.safeonline.entity.HistoryEntity" />
		<mapping class="net.link.safeonline.entity.SubscriptionEntity" />
	</session-factory>
</hibernate-configuration>