<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE datasources
        PUBLIC "-//JBoss//DTD JBOSS JCA Config 1.5//EN"
        "http://www.jboss.org/j2ee/dtd/jboss-ds_1_5.dtd">
<datasources>
	<local-tx-datasource>
		<jndi-name>SafeOnlineDS</jndi-name>
		<connection-url>
			jdbc:mysql://localhost:3306/safeonline
		</connection-url>
		<driver-class>com.mysql.jdbc.Driver</driver-class>
		<user-name>safeonline</user-name>
		<password>safeonline</password>
		<min-pool-size>5</min-pool-size>
		<max-pool-size>20</max-pool-size>
	</local-tx-datasource>
</datasources>