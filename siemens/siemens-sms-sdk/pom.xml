<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>net.lin-k.safe-online</groupId>
		<artifactId>siemens</artifactId>
		<version>1.4</version>
	</parent>

	<name>Siemens SMS SDK</name>
	<description>
		This SDK eases the communication with the Siemens SMS Web Service.
	</description>

	<groupId>net.lin-k.safe-online.siemens</groupId>
	<artifactId>siemens-sms-sdk</artifactId>
	<version>1.4</version>
	<packaging>jar</packaging>


	<dependencies>

		<!-- Dependency Groups -->
		<dependency>
			<groupId>net.lin-k.safe-online</groupId>
			<artifactId>safe-online-container-as</artifactId>
			<version>1.4</version>
			<type>pom</type>
		</dependency>

		<!-- Specific Dependencies: OLAS -->
		<dependency>
			<groupId>net.lin-k.safe-online.siemens</groupId>
			<artifactId>siemens-sms-ws</artifactId>
			<version>1.4</version>
		</dependency>

		<!-- Specific Dependencies: External -->

	</dependencies>

	<reporting>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-changes-plugin</artifactId>
				<reportSets>
					<reportSet>
						<reports>
							<report>changes-report</report>
						</reports>
					</reportSet>
				</reportSets>
			</plugin>
		</plugins>
	</reporting>

</project>
