<?xml version="1.0" encoding="UTF-8"?>
<endpoints xmlns="http://java.sun.com/xml/ns/jax-ws/ri/runtime"
	version="2.0">

	<endpoint name="SafeOnline-Attribute-WS"
		implementation="net.link.safeonline.attrib.ws.SAMLAttributePortImpl"
		url-pattern="/attrib"
		interface="oasis.names.tc.saml._2_0.protocol.SAMLAttributePort"
		service="{urn:oasis:names:tc:SAML:2.0:protocol}SAMLAttributeService"
		port="{urn:oasis:names:tc:SAML:2.0:protocol}SAMLAttributePort" />

	<endpoint name="SafeOnline-Data-WS"
		implementation="net.link.safeonline.data.ws.DataServicePortImpl"
		url-pattern="/data"
		interface="liberty.dst._2006_08.ref.safe_online.DataServicePort"
		service="{urn:liberty:dst:2006-08:ref:safe-online}DataService"
		port="{urn:liberty:dst:2006-08:ref:safe-online}DataServicePort" />

	<endpoint name="SafeOnline-Identifier-Mapping-WS"
		implementation="net.link.safeonline.idmapping.ws.NameIdentifierMappingPortImpl"
		url-pattern="/idmapping"
		interface="oasis.names.tc.saml._2_0.protocol.NameIdentifierMappingPort"
		service="{urn:oasis:names:tc:SAML:2.0:protocol}NameIdentifierMappingService"
		port="{urn:oasis:names:tc:SAML:2.0:protocol}NameIdentifierMappingPort" />

	<endpoint name="SafeOnline-Security-Token-Service"
		implementation="net.link.safeonline.sts.ws.SecurityTokenServicePortImpl"
		url-pattern="/sts"
		interface="org.oasis_open.docs.ws_sx.ws_trust._200512.SecurityTokenServicePort"
		service="{http://docs.oasis-open.org/ws-sx/ws-trust/200512/}SecurityTokenService"
		port="{http://docs.oasis-open.org/ws-sx/ws-trust/200512/}SecurityTokenServicePort" />

</endpoints>
