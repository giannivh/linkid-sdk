	<modelVersion>4.0.0</modelVersion>
	<groupId>net.lin-k.safe-online</groupId>
	<artifactId>safe-online-base-distribution</artifactId>
	<name>SafeOnline Base Distribution</name>
	<version>1.0-SNAPSHOT</version>
	<packaging>pom</packaging>
	<build>
		<resources>
			<resource>
				<directory>src/main</directory>
				<filtering>true</filtering>
			</resource>
		</resources>
		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>keytool-maven-plugin</artifactId>
				<executions>
					<execution>
						<phase>generate-resources</phase>
						<goals>
							<goal>clean</goal>
							<goal>genkey</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<keystore> ${project.build.directory}/files/ssl.jks </keystore>
					<dname> cn=SafeOnline Development, ou=Development,
						L=SINT-MARTENS-LATEM, ST=VL, o=LIN.K_NV, c=BE </dname>
					<keypass>secret</keypass>
					<storepass>secret</storepass>
					<alias>safe-online</alias>
					<verbose>true</verbose>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-resources-plugin</artifactId>
				<executions>
					<execution>
						<id>filter</id>
						<phase>process-resources</phase>
						<goals>
							<goal>resources</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
				<executions>
					<execution>
						<id>unpack</id>
						<phase>package</phase>
						<goals>
							<goal>unpack</goal>
						</goals>
						<configuration>
							<artifactItems>
								<artifactItem>
									<groupId>org.jboss</groupId>
									<artifactId>jboss-as</artifactId>
									<type>zip</type>
								</artifactItem>
								<artifactItem>
									<groupId>net.java.dev</groupId>
									<artifactId>jaxws-ri</artifactId>
									<type>zip</type>
								</artifactItem>
							</artifactItems>
							<outputDirectory> ${project.build.directory}/files 
							</outputDirectory>
						</configuration>
					</execution>
					<execution>
						<id>copy</id>
						<phase>package</phase>
						<goals>
							<goal>copy-dependencies</goal>
						</goals>
						<configuration>
							<outputDirectory> ${project.build.directory}/dependency 
							</outputDirectory>
							<excludeArtifactIds> jboss-as,jaxws-ri </excludeArtifactIds>
							<includeScope>compile</includeScope>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-assembly-plugin</artifactId>
				<configuration>
					<descriptors>
						<descriptor> src/main/assembly/jboss.xml </descriptor>
					</descriptors>
				</configuration>
				<executions>
					<execution>
						<id>package</id>
						<phase>package</phase>
						<goals>
							<goal>attached</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
