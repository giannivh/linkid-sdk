<?xml version='1.0'?>
<!DOCTYPE policy PUBLIC
      "-//JBoss//DTD JBOSS Security Config 3.0//EN"
      "http://www.jboss.org/j2ee/dtd/security_config.dtd">
<policy>

	<application-policy name="safe-online-demo">
		<authentication>
			<login-module
				code="net.link.safeonline.sdk.auth.jaas.SafeOnlineAuthenticationLoginModule"
				flag="required">
				<module-option name="location">
					localhost:8080
				</module-option>
			</login-module>
		</authentication>
	</application-policy>

</policy>
