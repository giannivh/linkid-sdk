<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>net.lin-k</groupId>
		<artifactId>safe-online</artifactId>
		<version>1.0.0-SNAPSHOT</version>
	</parent>
	<groupId>net.lin-k.safe-online</groupId>
	<artifactId>safe-online-deploy</artifactId>
	<packaging>ear</packaging>
	<version>1.0-SNAPSHOT</version>
	<name>SafeOnline Deploy</name>
	<description>SafeOnline Deployable Application Package</description>
	<build>
		<finalName>SafeOnline</finalName>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-ear-plugin</artifactId>
				<configuration>
					<version>1.4</version>
					<modules>
						<ejbModule>
							<groupId>net.lin-k.safe-online</groupId>
							<artifactId>safe-online-entity</artifactId>
						</ejbModule>
						<ejbModule>
							<groupId>net.lin-k.safe-online</groupId>
							<artifactId>safe-online-model</artifactId>
						</ejbModule>
						<webModule>
							<groupId>net.lin-k.safe-online</groupId>
							<artifactId>safe-online-startup</artifactId>
						</webModule>
						<webModule>
							<groupId>net.lin-k.safe-online</groupId>
							<artifactId>
								safe-online-ws-runtime
							</artifactId>
							<contextRoot>/safe-online-ws</contextRoot>
						</webModule>
						<javaModule>
							<groupId>org.jboss.seam</groupId>
							<artifactId>jboss-seam</artifactId>
							<includeInApplicationXml>
								true
							</includeInApplicationXml>
						</javaModule>
						<ejbModule>
							<groupId>net.lin-k.safe-online</groupId>
							<artifactId>safe-online-user</artifactId>
						</ejbModule>
						<webModule>
							<groupId>net.lin-k.safe-online</groupId>
							<artifactId>
								safe-online-user-webapp
							</artifactId>
							<contextRoot>/safe-online</contextRoot>
						</webModule>
						<sarModule>
							<groupId>net.lin-k.safe-online</groupId>
							<artifactId>
								safe-online-user-service
							</artifactId>
						</sarModule>
						<sarModule>
							<groupId>net.lin-k.safe-online</groupId>
							<artifactId>safe-online-service</artifactId>
						</sarModule>
						<javaModule>
							<groupId>net.lin-k.safe-online</groupId>
							<artifactId>
								safe-online-j2ee-util
							</artifactId>
							<includeInApplicationXml>
								true
							</includeInApplicationXml>
						</javaModule>
						<ejbModule>
							<groupId>net.lin-k.safe-online</groupId>
							<artifactId>safe-online-oper</artifactId>
						</ejbModule>
						<webModule>
							<groupId>net.lin-k.safe-online</groupId>
							<artifactId>
								safe-online-oper-webapp
							</artifactId>
							<contextRoot>/safe-online-oper</contextRoot>
						</webModule>
						<sarModule>
							<groupId>net.lin-k.safe-online</groupId>
							<artifactId>
								safe-online-oper-service
							</artifactId>
						</sarModule>
						<ejbModule>
							<groupId>net.lin-k.safe-online</groupId>
							<artifactId>safe-online-owner</artifactId>
						</ejbModule>
						<webModule>
							<groupId>net.lin-k.safe-online</groupId>
							<artifactId>
								safe-online-owner-webapp
							</artifactId>
							<contextRoot>
								/safe-online-owner
							</contextRoot>
						</webModule>
						<sarModule>
							<groupId>net.lin-k.safe-online</groupId>
							<artifactId>
								safe-online-owner-service
							</artifactId>
						</sarModule>
					</modules>
					<jboss>
						<version>4</version>
					</jboss>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>jboss-maven-plugin</artifactId>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<dependency>
			<groupId>net.lin-k.safe-online</groupId>
			<artifactId>safe-online-container</artifactId>
			<type>pom</type>
			<version>1.0-SNAPSHOT</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>net.lin-k.safe-online</groupId>
			<artifactId>safe-online-entity</artifactId>
			<type>ejb</type>
			<version>1.0-SNAPSHOT</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>net.lin-k.safe-online</groupId>
			<artifactId>safe-online-model</artifactId>
			<type>ejb</type>
			<version>1.0-SNAPSHOT</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>net.lin-k.safe-online</groupId>
			<artifactId>safe-online-startup</artifactId>
			<type>war</type>
			<version>1.0-SNAPSHOT</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>net.lin-k.safe-online</groupId>
			<artifactId>safe-online-ws-runtime</artifactId>
			<type>war</type>
			<version>1.0-SNAPSHOT</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>net.lin-k.safe-online</groupId>
			<artifactId>safe-online-user</artifactId>
			<type>ejb</type>
			<version>1.0-SNAPSHOT</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>net.lin-k.safe-online</groupId>
			<artifactId>safe-online-user-webapp</artifactId>
			<type>war</type>
			<version>1.0-SNAPSHOT</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.jboss.seam</groupId>
			<artifactId>jboss-seam</artifactId>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>net.lin-k.safe-online</groupId>
			<artifactId>safe-online-user-service</artifactId>
			<type>sar</type>
			<version>1.0-SNAPSHOT</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>net.lin-k.safe-online</groupId>
			<artifactId>safe-online-service</artifactId>
			<type>sar</type>
			<version>1.0-SNAPSHOT</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>net.lin-k.safe-online</groupId>
			<artifactId>safe-online-j2ee-util</artifactId>
			<version>1.0-SNAPSHOT</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>net.lin-k.safe-online</groupId>
			<artifactId>safe-online-oper-webapp</artifactId>
			<type>war</type>
			<version>1.0-SNAPSHOT</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>net.lin-k.safe-online</groupId>
			<artifactId>safe-online-oper</artifactId>
			<type>ejb</type>
			<version>1.0-SNAPSHOT</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>net.lin-k.safe-online</groupId>
			<artifactId>safe-online-oper-service</artifactId>
			<type>sar</type>
			<version>1.0-SNAPSHOT</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>net.lin-k.safe-online</groupId>
			<artifactId>safe-online-owner-webapp</artifactId>
			<type>war</type>
			<version>1.0-SNAPSHOT</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>net.lin-k.safe-online</groupId>
			<artifactId>safe-online-owner</artifactId>
			<type>ejb</type>
			<version>1.0-SNAPSHOT</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>net.lin-k.safe-online</groupId>
			<artifactId>safe-online-owner-service</artifactId>
			<type>sar</type>
			<version>1.0-SNAPSHOT</version>
			<scope>provided</scope>
		</dependency>
	</dependencies>
	<reporting>
		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>changes-maven-plugin</artifactId>
				<reportSets>
					<reportSet>
						<reports>
							<report>changes-report</report>
						</reports>
					</reportSet>
				</reportSets>
			</plugin>
		</plugins>
	</reporting>
</project>
