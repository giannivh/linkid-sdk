<?xml version="1.0" encoding="UTF-8"?>
<pkcs11-sc-config xmlns="urn:net:lin-k:safe-online:pkcs11-sc-config:1.0"
	alias="test-alias" name="Test Smart Card Config">
	<atrs>
		<atr>
			<byte>0x01</byte>
			<byte>0x02</byte>
			<byte>0x03</byte>
			<byte>0x04</byte>
			<byte>0x05</byte>
			<byte>0x06</byte>
