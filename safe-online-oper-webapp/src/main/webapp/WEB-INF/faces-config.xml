<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>

	<application>
		<message-bundle>messages.webapp</message-bundle>
		<locale-config>
			<default-locale>en</default-locale>
			<supported-locale>nl</supported-locale>
		</locale-config>
		<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
	</application>

	<lifecycle>
		<phase-listener>
			org.jboss.seam.jsf.SeamPhaseListener
		</phase-listener>
	</lifecycle>

	<managed-bean>
		<managed-bean-name>attributeViewTabControl</managed-bean-name>
		<managed-bean-class>
			net.link.safeonline.oper.bean.AttributeViewTabControlBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<navigation-rule>
		<navigation-case>
			<from-outcome>register</from-outcome>
			<to-view-id>/register.seam</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>login-success</from-outcome>
			<to-view-id>/overview.seam</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>logout-success</from-outcome>
			<to-view-id>/main.seam</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/applications.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>add-application</from-outcome>
			<to-view-id>/application-add.seam</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>view</from-outcome>
			<to-view-id>/application-view.seam</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/application-add.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/applications.seam</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>cancel</from-action>
			<to-view-id>/applications.seam</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/application-view.xhtml</from-view-id>
		<navigation-case>
			<from-action>back</from-action>
			<to-view-id>/applications.seam</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>edit</from-action>
			<to-view-id>/application-edit.seam</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/application-edit.xhtml</from-view-id>
		<navigation-case>
			<from-action>cancel</from-action>
			<to-view-id>/application-view.seam</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/application-view.seam</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/owners.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>add-owner</from-outcome>
			<to-view-id>/owner-add.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/owner-add.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/owners.seam</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>cancel</from-action>
			<to-view-id>/owners.seam</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pkix.xhtml</from-view-id>
		<navigation-case>
			<from-action>add</from-action>
			<to-view-id>/trust-domain-add.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>view</from-outcome>
			<to-view-id>/trust-domain.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>removed</from-outcome>
			<to-view-id>/pkix.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>clear-cache</from-outcome>
			<to-view-id>/pkix.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/trust-domain-add.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/pkix.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>cancel</from-action>
			<to-view-id>/pkix.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/trust-domain.xhtml</from-view-id>
		<navigation-case>
			<from-action>back</from-action>
			<to-view-id>/pkix.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>add</from-action>
			<to-view-id>/trust-point-add.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>view</from-outcome>
			<to-view-id>/trust-point-view.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/trust-domain-edit.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>clear-cache</from-outcome>
			<to-view-id>/trust-domain.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/trust-domain-edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/trust-domain.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>canceled</from-outcome>
			<to-view-id>/trust-domain.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/trust-point-add.xhtml</from-view-id>
		<navigation-case>
			<from-action>cancel</from-action>
			<to-view-id>/trust-domain.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/trust-domain.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/trust-point-view.xhtml</from-view-id>
		<navigation-case>
			<from-action>back</from-action>
			<to-view-id>/trust-domain.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>removed</from-outcome>
			<to-view-id>/trust-domain.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/tasks.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>taskview</from-outcome>
			<to-view-id>/task-view.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>schedulingview</from-outcome>
			<to-view-id>/scheduling-view.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>schedulinglistview</from-outcome>
			<to-view-id>/schedulings.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>successclearalltaskshistory</from-outcome>
			<to-view-id>/schedulings.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/task-view.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>tasklistview</from-outcome>
			<to-view-id>/tasks.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>schedulingview</from-outcome>
			<to-view-id>/scheduling-view.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>taskhistoryview</from-outcome>
			<to-view-id>/task-history.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>successcleartaskhistory</from-outcome>
			<to-view-id>/task-view.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>successperformtask</from-outcome>
			<to-view-id>/task-history.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>taskeditview</from-outcome>
			<to-view-id>/task-edit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/task-edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>taskview</from-outcome>
			<to-view-id>/task-view.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>successsavetask</from-outcome>
			<to-view-id>/task-view.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/schedulings.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>tasklistview</from-outcome>
			<to-view-id>/tasks.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>schedulingview</from-outcome>
			<to-view-id>/scheduling-view.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>addschedulingview</from-outcome>
			<to-view-id>/scheduling-add.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/scheduling-view.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>schedulinglistview</from-outcome>
			<to-view-id>/schedulings.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>taskview</from-outcome>
			<to-view-id>/task-view.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>tasklistview</from-outcome>
			<to-view-id>/tasks.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>schedulingedit</from-outcome>
			<to-view-id>/scheduling-edit.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>successperformscheduling</from-outcome>
			<to-view-id>/scheduling-view.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/scheduling-edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>schedulingview</from-outcome>
			<to-view-id>/scheduling-view.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>successsavescheduling</from-outcome>
			<to-view-id>/scheduling-view.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/scheduling-add.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>schedulinglistview</from-outcome>
			<to-view-id>/schedulings.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>successaddscheduling</from-outcome>
			<to-view-id>/schedulings.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/task-history.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>taskview</from-outcome>
			<to-view-id>/task-view.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>successcleartaskhistory</from-outcome>
			<to-view-id>/task-history.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/attributes.xhtml</from-view-id>
		<navigation-case>
			<from-action>add</from-action>
			<to-view-id>/attribute-add.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>view</from-outcome>
			<to-view-id>/attribute-view.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/attribute-view.xhtml</from-view-id>
		<navigation-case>
			<from-action>back</from-action>
			<to-view-id>/attributes.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>add</from-action>
			<to-view-id>/attribute-desc-add.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/attribute-desc-edit.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>add-provider</from-action>
			<to-view-id>/attribute-provider-add.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>provider-removed</from-outcome>
			<to-view-id>/attribute-view.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/attribute-provider-add.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/attribute-view.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/attribute-view.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/attribute-desc-edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/attribute-view.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>saved</from-outcome>
			<to-view-id>/attribute-view.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>removed</from-outcome>
			<to-view-id>/attribute-view.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/attribute-desc-add.xhtml</from-view-id>
		<navigation-case>
			<from-action>cancel</from-action>
			<to-view-id>/attribute-view.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/attribute-view.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/attribute-add.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/attributes.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>cancel</from-action>
			<to-view-id>/attributes.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/configuration.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>saved</from-outcome>
			<to-view-id>/overview.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>cancel</from-action>
			<to-view-id>/overview.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
