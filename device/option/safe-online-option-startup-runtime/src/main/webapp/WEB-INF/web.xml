<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<display-name>SafeOnline Option Startup</display-name>

    <!-- Application Context Parameters -->
	<context-param>
		<description>The JNDI prefix used by startable components.</description>
		<param-name>StartableJndiPrefix</param-name>
		<param-value>SafeOnlineOption/startup</param-value>
	</context-param>

    <!-- Application Event Listeners -->
	<listener>
		<listener-class>net.link.safeonline.listener.StartupServletContextListener</listener-class>
	</listener>

</web-app>
