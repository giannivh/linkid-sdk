<?xml version='1.0' encoding='UTF-8'?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">
	<application>
		<resource-bundle>
			<base-name>messages.webapp</base-name>
			<var>msgs</var>
		</resource-bundle>
		<locale-config>
			<default-locale>en</default-locale>
			<supported-locale>en</supported-locale>
			<supported-locale>nl</supported-locale>
		</locale-config>
		<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
	</application>
	<lifecycle>
		<phase-listener> org.jboss.seam.jsf.SeamPhaseListener 
		</phase-listener>
	</lifecycle>
	<navigation-rule>
		<from-view-id>/admin.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>search</from-outcome>
			<to-view-id>/search.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/search.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/search-result.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/admin.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/search-result.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/search.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/search-result.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/prescription-new.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/care-provider.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/prescription-authenticated.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>created</from-outcome>
			<to-view-id>/care-provider.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/prescription-fill.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/pharmacist.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id> /prescription-fill-authenticated.xhtml </from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/prescription-fill.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>view</from-outcome>
			<to-view-id>/prescription-fill-view.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/prescription-fill-view.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id> /prescription-fill-authenticated.xhtml </to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>filled</from-outcome>
			<to-view-id> /prescription-fill-authenticated.xhtml </to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/roles.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>admin</from-outcome>
			<to-view-id>/admin.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>careProvider</from-outcome>
			<to-view-id>/care-provider.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>pharmacist</from-outcome>
			<to-view-id>/pharmacist.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/patient.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>view</from-outcome>
			<to-view-id>/prescription-view.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/prescription-view.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/patient.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>