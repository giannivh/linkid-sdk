<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">
	<application>
		<resource-bundle>
			<base-name>messages.webapp</base-name>
			<var>msgs</var>
		</resource-bundle>
		<locale-config>
			<default-locale>en</default-locale>
			<supported-locale>en</supported-locale>
			<supported-locale>nl</supported-locale>
		</locale-config>
		<view-handler>com.sun.facelets.FaceletViewHandler 
		</view-handler>
	</application>
	<lifecycle>
		<phase-listener> org.jboss.seam.jsf.SeamPhaseListener</phase-listener>
		<phase-listener>net.link.safeonline.auth.listener.LanguagePhaseListener</phase-listener>
	</lifecycle>
	<navigation-rule>
		<from-view-id>/help.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>create-ticket</from-outcome>
			<to-view-id>/help_ticket.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/first-time.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>new</from-outcome>
			<to-view-id>/new-user.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>existing</from-outcome>
			<to-view-id>/main.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/new-user.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>next</from-outcome>
			<to-view-id>/new-user-device.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>existing</from-outcome>
			<to-view-id>/main.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/identity-confirmation.xhtml 
		</from-view-id>
		<navigation-case>
			<from-outcome>reject</from-outcome>
			<to-view-id>/identity-rejection.xhtml 
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>missing-attributes</from-outcome>
			<to-view-id>/missing-attributes.xhtml 
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>identity-unavailable</from-outcome>
			<to-view-id>/identity-unavailable.xhtml 
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/subscription.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>reject</from-outcome>
			<to-view-id>/subscription-rejection.xhtml 
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>confirmation-required 
			</from-outcome>
			<to-view-id>/identity-confirmation.xhtml 
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>missing-attributes</from-outcome>
			<to-view-id>/missing-attributes.xhtml 
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/global-confirmation.xhtml 
		</from-view-id>
		<navigation-case>
			<from-outcome>reject</from-outcome>
			<to-view-id> /global-confirmation-rejection.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>subscription-required 
			</from-outcome>
			<to-view-id>/subscription.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>confirmation-required 
			</from-outcome>
			<to-view-id>/identity-confirmation.xhtml 
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>missing-attributes</from-outcome>
			<to-view-id>/missing-attributes.xhtml 
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/register-device.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/all-devices.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>