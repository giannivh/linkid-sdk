<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>

	<application>
		<message-bundle>messages.webapp</message-bundle>
		<locale-config>
			<default-locale>en</default-locale>
			<supported-locale>en</supported-locale>
			<supported-locale>nl</supported-locale>
		</locale-config>
		<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
	</application>

	<lifecycle>
		<phase-listener>
			org.jboss.seam.jsf.SeamPhaseListener
		</phase-listener>
	</lifecycle>

	<navigation-rule>
		<from-view-id>/first-time.xhtml</from-view-id>
		<navigation-case>
			<from-action>new</from-action>
			<to-view-id>/new-user.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>existing</from-action>
			<to-view-id>/main.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/new-user.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>next</from-outcome>
			<to-view-id>/new-user-device.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/identity-confirmation.xhtml</from-view-id>
		<navigation-case>
			<from-action>reject</from-action>
			<to-view-id>/identity-rejection.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>missing-attributes</from-outcome>
			<to-view-id>/missing-attributes.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/password/username-password.xhtml</from-view-id>
		<navigation-case>
			<from-action>back</from-action>
			<to-view-id>/main.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>help</from-action>
			<to-view-id>/help.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/password/register-password.xhtml</from-view-id>
		<navigation-case>
			<from-action>help</from-action>
			<to-view-id>/help.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/encap/register-mobile.xhtml</from-view-id>
		<navigation-case>
			<from-action>help</from-action>
			<to-view-id>/help.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/beid/register-beid.xhtml</from-view-id>
		<navigation-case>
			<from-action>help</from-action>
			<to-view-id>/help.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/password/new-user-password.xhtml</from-view-id>
		<navigation-case>
			<from-action>help</from-action>
			<to-view-id>/help.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/beid/new-user-beid.xhtml</from-view-id>
		<navigation-case>
			<from-action>help</from-action>
			<to-view-id>/help.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/encap/new-user-mobile.xhtml</from-view-id>
		<navigation-case>
			<from-action>help</from-action>
			<to-view-id>/help.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/subscription.xhtml</from-view-id>
		<navigation-case>
			<from-action>reject</from-action>
			<to-view-id>/subscription-rejection.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>confirmation-required</from-outcome>
			<to-view-id>/identity-confirmation.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>missing-attributes</from-outcome>
			<to-view-id>/missing-attributes.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/global-confirmation.xhtml</from-view-id>
		<navigation-case>
			<from-action>reject</from-action>
			<to-view-id>
				/global-confirmation-rejection.xhtml
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>subscription-required</from-outcome>
			<to-view-id>/subscription.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>confirmation-required</from-outcome>
			<to-view-id>/identity-confirmation.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>missing-attributes</from-outcome>
			<to-view-id>/missing-attributes.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
