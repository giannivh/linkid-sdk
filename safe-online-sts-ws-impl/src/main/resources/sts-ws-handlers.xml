<?xml version="1.0" encoding="UTF-8"?>
<jws:handler-chains xmlns:jws="http://java.sun.com/xml/ns/javaee">
	<jws:handler-chain>
		<jws:handler>
			<jws:handler-class>
				net.link.safeonline.sts.ws.TokenValidationHandler
			</jws:handler-class>
		</jws:handler>
		<jws:handler>
			<jws:handler-class>
				net.link.safeonline.ws.util.ApplicationLoginHandler
			</jws:handler-class>
		</jws:handler>
		<jws:handler>
			<jws:handler-class>
				net.link.safeonline.ws.util.ApplicationCertificateMapperHandler
			</jws:handler-class>
		</jws:handler>
		<jws:handler>
			<jws:handler-class>
				net.link.safeonline.ws.util.ApplicationCertificateValidatorHandler
			</jws:handler-class>
		</jws:handler>
		<jws:handler>
			<jws:handler-class>
				net.link.safeonline.ws.util.WSSecurityServerHandler
			</jws:handler-class>
		</jws:handler>
	</jws:handler-chain>
</jws:handler-chains>
