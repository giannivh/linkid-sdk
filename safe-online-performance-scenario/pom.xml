<?xml version="1.0" encoding="UTF-8"?><project>
  <parent>
    <artifactId>safe-online</artifactId>
    <groupId>net.lin-k</groupId>
    <version>1.0.0-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>net.lin-k.safe-online</groupId>
  <artifactId>safe-online-performance-scenario</artifactId>
  <packaging>ejb</packaging>
  <name>SafeOnline Performance Scenario</name>
  <version>1.0-SNAPSHOT</version>
  <description>Scenario for testing SafeOnline's performance.</description>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-ejb-plugin</artifactId>
        <configuration>
          <ejbVersion>3.0</ejbVersion>
          <archive>
            <manifest>
              <addClasspath>true</addClasspath>
            </manifest>
          </archive>
          <generateClient>true</generateClient>
          <clientExcludes>
              <clientExclude>**/*Bean.class</clientExclude>
              <clientExclude>**/*CMP.class</clientExclude>
              <clientExclude>**/*Session.class</clientExclude>
              <clientExclude>**/package.html</clientExclude>
              <clientExclude>**/*Scenario.class</clientExclude>
              <clientExclude>META-INF/ejb-jar.xml</clientExclude>
          </clientExcludes>
        </configuration>
      </plugin>
    </plugins>
    <resources>
        <resource>
            <directory>${basedir}/src/main/resources</directory>
            <filtering>true</filtering>
        </resource>
    </resources>
  </build>
  <dependencies>
    <dependency>
      <groupId>net.lin-k.safe-online</groupId>
      <artifactId>safe-online-container</artifactId>
      <version>1.0-SNAPSHOT</version>
      <type>pom</type>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>net.lin-k.safe-online</groupId>
      <artifactId>safe-online-performance-entity</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>
    <dependency>
      <groupId>net.lin-k.safe-online</groupId>
      <artifactId>safe-online-performance-scenario-base</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>
    <dependency>
      <groupId>net.lin-k.safe-online</groupId>
      <artifactId>safe-online-model-performance</artifactId>
      <version>1.0-SNAPSHOT</version>
      <type>ejb-client</type>
      <exclusions>
        <exclusion>
          <groupId>net.lin-k.safe-online</groupId>
          <artifactId>safe-online-model</artifactId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>net.lin-k.safe-online</groupId>
      <artifactId>safe-online-performance-keystore</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>
    <dependency>
      <groupId>commons-logging</groupId>
      <artifactId>commons-logging-api</artifactId>
      <exclusions>
        <exclusion>
          <groupId>javax.servlet</groupId>
          <artifactId>servlet-api</artifactId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
        <groupId>jgroups</groupId>
        <artifactId>jgroups</artifactId>
    </dependency>
    <dependency>
      <groupId>jfree</groupId>
      <artifactId>jfreechart</artifactId>
    </dependency>
    <dependency>
      <groupId>jfree</groupId>
      <artifactId>jcommon</artifactId>
    </dependency>
  </dependencies>
  <reporting>
    <plugins>
      <plugin>
        <artifactId>maven-changes-plugin</artifactId>
        <reportSets>
          <reportSet>
            <reports>
              <report>changes-report</report>
            </reports>
          </reportSet>
        </reportSets>
      </plugin>
      <plugin>
        <artifactId>maven-javadoc-plugin</artifactId>
      </plugin>
      <plugin>
        <artifactId>maven-surefire-report-plugin</artifactId>
      </plugin>
    </plugins>
  </reporting>
</project>
