	<artifactId>safe-online-demo-lawyer-webapp</artifactId>
	<packaging>war</packaging>
	<version>1.0-SNAPSHOT</version>
	<name>SafeOnline Demo Lawyer Web Application</name>
	<description>
		Demo Lawyer Web Application using SafeOnline.
	</description>
	<profiles>
		<profile>
			<id>env-dev</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<auth.webapp.location>
					http://localhost:8080/safe-online-auth/
				</auth.webapp.location>
				<local.host.name>localhost</local.host.name>
			</properties>
		</profile>
		<profile>
			<id>env-buildserver</id>
			<activation>
				<property>
					<name>env</name>
					<value>buildserver</value>
				</property>
			</activation>
			<properties>
				<auth.webapp.location>
					http://buildserver:8080/safe-online-auth/
				</auth.webapp.location>
				<local.host.name>buildserver</local.host.name>
			</properties>
		</profile>
	</profiles>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<configuration>
					<webResources>
						<resource>
							<directory>
								${basedir}/src/main/webapp/WEB-INF
							</directory>
							<targetPath>WEB-INF</targetPath>
							<filtering>true</filtering>
						</resource>
					</webResources>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>jboss-maven-plugin</artifactId>
			</plugin>
		</plugins>
	</build>
