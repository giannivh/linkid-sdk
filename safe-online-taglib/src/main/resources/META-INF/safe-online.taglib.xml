<!DOCTYPE facelet-taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
  "https://facelets.dev.java.net/source/browse/*checkout*/facelets/src/etc/facelet-taglib_1_0.dtd">
<facelet-taglib xmlns="http://java.sun.com/JSF/Facelet">

	<namespace>urn:net:lin-k:safe-online:taglib:1.0</namespace>

	<tag>
		<tag-name>echo</tag-name>
		<source>tags/echo.xhtml</source>
	</tag>

	<tag>
		<tag-name>nbsp</tag-name>
		<source>tags/nbsp.xhtml</source>
	</tag>

	<tag>
		<tag-name>outputAttribute</tag-name>
		<component>
			<component-type>
				net.link.component.attributeOutput
			</component-type>
		</component>
	</tag>

	<tag>
		<tag-name>outputAttributeName</tag-name>
		<component>
			<component-type>
				net.link.component.attributeNameOutput
			</component-type>
		</component>
	</tag>

	<tag>
		<tag-name>inputAttribute</tag-name>
		<component>
			<component-type>
				net.link.component.attributeInput
			</component-type>
		</component>
	</tag>

</facelet-taglib>