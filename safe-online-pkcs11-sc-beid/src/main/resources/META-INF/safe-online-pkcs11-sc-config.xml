<?xml version="1.0" encoding="UTF-8"?>
<pkcs11-sc-config xmlns="urn:net:lin-k:safe-online:pkcs11-sc-config:1.0"
	alias="beid" name="Belgium eID">
	<atrs>
		<atr>
			<byte>0x3B</byte>
			<byte>0x98</byte>
			<byte>0x94</byte>
			<byte>0x40</byte>
			<byte>0x0A</byte>
			<byte>0xA5</byte>
			<byte>0x03</byte>
			<byte>0x01</byte>
			<byte>0x01</byte>
			<byte>0x01</byte>
			<byte>0xAD</byte>
			<byte>0x13</byte>
			<byte>0x10</byte>
		</atr>
		<atr>
			<byte>0x3B</byte>
			<byte>0x98</byte>
			<byte>0x13</byte>
			<byte>0x40</byte>
			<byte>0x0A</byte>
			<byte>0xA5</byte>
			<byte>0x03</byte>
			<byte>0x01</byte>
			<byte>0x01</byte>
			<byte>0x01</byte>
			<byte>0xAD</byte>
			<byte>0x13</byte>
			<byte>0x11</byte>
		</atr>
	</atrs>
	<pkcs11-keystore-mapping>
		<key type="AUTHENTICATION" alias="Authentication" />
		<key type="SIGNATURE" alias="Signature" />
	</pkcs11-keystore-mapping>
	<pkcs11-library>
		<platform match="Linux" name="Linux">
			<location>/usr/local/lib/libbeidpkcs11.so</location>
			<location>/usr/lib/libbeidpkcs11.so</location>
		</platform>
		<platform match="Windows XP" name="Microsoft Windows XP">
			<location>
				C:\WINDOWS\system32\Belgium Identity Card PKCS11.dll
			</location>
		</platform>
		<platform match="Windows 2000" name="Microsoft Windows 2000">
			<location>
				C:\WINNT\system32\Belgium Identity Card PKCS11.dll
			</location>
		</platform>
		<platform match="Windows Vista"
			name="Microsoft Windows Vista">
			<location>
				C:\WINDOWS\system32\Belgium Identity Card PKCS11.dll
			</location>
		</platform>
		<platform match="Windows.*" name="Microsoft Windows Platform">
			<location>
				C:\WINDOWS\system32\Belgium Identity Card PKCS11.dll
			</location>
		</platform>
		<platform match="Mac OS X" name="Apple Mac OS X">
			<location>
				/usr/local/lib/beid-pkcs11.bundle/Contents/MacOS/libbeidpkcs11.2.1.0.dylib
			</location>
		</platform>
	</pkcs11-library>
	<identity-extractor
		classname="net.link.safeonline.beid.BeIdentityExtractor" />
</pkcs11-sc-config>