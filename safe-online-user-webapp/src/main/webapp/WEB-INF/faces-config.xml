<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>

	<application>
		<message-bundle>messages.webapp</message-bundle>
		<locale-config>
			<default-locale>en</default-locale>
			<supported-locale>en</supported-locale>
			<supported-locale>nl</supported-locale>
		</locale-config>
		<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
	</application>

	<lifecycle>
		<phase-listener>
			org.jboss.seam.jsf.SeamPhaseListener
		</phase-listener>
	</lifecycle>

	<navigation-rule>
		<navigation-case>
			<from-outcome>login-success</from-outcome>
			<to-view-id>/overview.seam</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>logout-success</from-outcome>
			<to-view-id>/main.seam</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>profile</from-action>
			<to-view-id>/profile/profile.seam</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/profile/profile.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/profile/attribute-edit.seam</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>removed</from-outcome>
			<to-view-id>/profile/profile.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>add</from-outcome>
			<to-view-id>/profile/attribute-add.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/profile/attribute-add.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/profile/profile.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>cancel</from-action>
			<to-view-id>/profile/profile.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/profile/attribute-edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/profile/profile.seam</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>cancel</from-action>
			<to-view-id>/profile/profile.seam</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/register.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/register-complete.seam</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/applications.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>view-subscription</from-outcome>
			<to-view-id>/view-subscription.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>unsubscribed</from-outcome>
			<to-view-id>/applications.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/view-subscription.xhtml</from-view-id>
		<navigation-case>
			<from-action>back</from-action>
			<to-view-id>/applications.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/view-application.xhtml</from-view-id>
		<navigation-case>
			<from-action>back</from-action>
			<to-view-id>/applications.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/devices.xhtml</from-view-id>
		<navigation-case>
			<from-action>extract</from-action>
			<to-view-id>/extract-identity.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/passwd-confirm.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/passwd-confirm.xhtml</from-view-id>
		<navigation-case>
			<from-action>back</from-action>
			<to-view-id>/devices.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/actions.xhtml</from-view-id>
		<navigation-case>
			<from-action>remove</from-action>
			<to-view-id>/remove.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>merge</from-action>
			<to-view-id>/merge/merge.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/remove.xhtml</from-view-id>
		<navigation-case>
			<from-action>cancel</from-action>
			<to-view-id>/actions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/merge/merge.xhtml</from-view-id>
		<navigation-case>
			<from-action>merge-login</from-action>
			<to-view-id>/merge/merge-login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/merge/merge-login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>password</from-outcome>
			<to-view-id>/merge/username-password.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>beid</from-outcome>
			<to-view-id>/merge/beid.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>mobileWeak</from-outcome>
			<to-view-id>/merge/mobile.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>mobileStrong</from-outcome>
			<to-view-id>/merge/mobile.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/merge/username-password.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/merge/merge-overview.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>back</from-action>
			<to-view-id>/merge/merge-login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>help</from-action>
			<to-view-id>/help.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/merge/beid.xhtml</from-view-id>
		<navigation-case>
			<from-action>help</from-action>
			<to-view-id>/help.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/merge/mobile.xhtml</from-view-id>
		<navigation-case>
			<from-action>back</from-action>
			<to-view-id>/merge/merge-login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>help</from-action>
			<to-view-id>/help.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
