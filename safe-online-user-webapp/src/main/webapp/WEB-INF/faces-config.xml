<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">
	<application>
		<resource-bundle>
			<base-name>messages.webapp</base-name>
			<var>msgs</var>
		</resource-bundle>
		<locale-config>
			<default-locale>en</default-locale>
			<supported-locale>en</supported-locale>
			<supported-locale>nl</supported-locale>
		</locale-config>
		<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
	</application>
	<lifecycle>
		<phase-listener> org.jboss.seam.jsf.SeamPhaseListener 
		</phase-listener>
		<phase-listener>net.link.safeonline.webapp.listener.LanguagePhaseListener</phase-listener>
	</lifecycle>
	<navigation-rule>
		<navigation-case>
			<from-outcome>login-success</from-outcome>
			<to-view-id>/overview.seam</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>logout-success</from-outcome>
			<to-view-id>/main.seam</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>profile</from-outcome>
			<to-view-id>/profile/profile.seam</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/profile/profile.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/profile/attribute-edit.seam</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>removed</from-outcome>
			<to-view-id>/profile/profile.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>add</from-outcome>
			<to-view-id>/profile/attribute-add.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/profile/attribute-add.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/profile/profile.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/profile/profile.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/profile/attribute-edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/profile/profile.seam</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/profile/profile.seam</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/register.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/register-complete.seam</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/applications.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>view-subscription</from-outcome>
			<to-view-id>/view-subscription.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>unsubscribed</from-outcome>
			<to-view-id>/applications.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/view-subscription.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/applications.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/view-application.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/applications.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id> /device/password/register-password.xhtml 
		</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id> /device/password/confirm-password.xhtml </to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/device/devices.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id> /device/password/confirm-password.xhtml </from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/device/devices.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id> /device/password/remove-password.xhtml </from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/device/devices.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/device/devices.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/account.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>remove</from-outcome>
			<to-view-id>/remove.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/remove.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/account.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
